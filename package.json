{
  "name": "burst-valve",
  "version": "1.2.0",
  "description": "An in memory queue for async processes in high concurrency code paths",
  "author": "Corey Hart <corey@codenothing.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/codenothing/burst-valve.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc -p tsconfig.dist.json",
    "lint": "eslint . --ext .ts",
    "pretest": "yarn build && yarn lint",
    "test": "jest --verbose --coverage",
    "prepublish": "yarn test",
    "benchmark": "./benchmark/run.sh"
  },
  "keywords": [
    "concurrency",
    "util"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/benchmark": "^2.1.2",
    "@types/jest": "^29.2.1",
    "@types/memcached": "^2.2.7",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "memcached": "^2.2.2",
    "mysql": "^2.18.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
